  <title><%= full_title(yield(:title)) %></title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="">
  <%= favicon_link_tag 'favicon.ico' %>

  <!-- Latest compiled and minified CSS -->

  <!-- Using the optional Bootstrap theme from CDN -->
  <%= stylesheet_link_tag "http://netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag "application" , media: "all", "data-turbolinks-track" => true %>

<!-- Import Font Awesome from the CDN -->
  <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">

<!-- javascript -->
 <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
<!-- EOS javascript (There will be more in the footer)  -->

  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>

  <!-- Include Jquery fade in method to the page -->
  <script>
    $(function()
    {
      $('body').hide().fadeIn(1000);
    });
  </script>
  <!-- End of JQuery -->

  <%= javascript_include_tag"//netdna.bootstrapcdn.com/bootstrap/3.0.2/js/bootstrap.min.js", "data-turbolinks-track" => true %>



  <!-- Prevent cross site request forgery -->
  <%= csrf_meta_tags %>
  <!-- End of csrf -->


<!-- Live reolad for development -->
    <% if Rails.env.development? %>
        <%= render 'shared/livereload' %>
    <% end %>
<!-- End of Live reload -->


